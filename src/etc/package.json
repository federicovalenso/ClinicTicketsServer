    "bootstrap-vue": "^2.0.0",
    "chart.js": "^2.8.0",
    "cross-env": "^5.2.0",
    "dateformat": "^3.0.3",
    "jquery": "^3.4.1",
    "laravel-mix": "^4.1.2",
    "popper.js": "^1.15.0",
    "vue": "^2.6.10",
    "vue-chartjs": "^3.4.2"
  },
  "devDependencies": {
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.22.10",
    "sass-loader": "^7.3.1",
    "vue-template-compiler": "^2.6.10"
