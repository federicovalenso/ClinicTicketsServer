    "chart.js": "^2.9.3",
    "cross-env": "^5.2.1",
    "dateformat": "^3.0.3",
    "jquery": "^3.5.0",
    "laravel-mix": "^4.1.4",
    "popper.js": "^1.16.1",
    "vue": "^2.6.11",
    "vue-chartjs": "^3.5.0"
  },
  "devDependencies": {
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.26.2",
